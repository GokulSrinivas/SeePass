{
  "manifest_version": 2,

  "name": "See Pass",
  "description": "This extension shows your passwords.",
  "version": "1.0",
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentscript.js"]
    }],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
